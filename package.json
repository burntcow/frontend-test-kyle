{
  "name": "front-end-test",
  "version": "1.0.0",
  "description": "A test for prospective front-end developers",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls"
  },
  "author": "John Deere",
  "license": "Copyright Â© 2017 Deere & Company",
  "dependencies": {
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jest-css-modules": "^1.1.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.2",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-babel-jest": "^1.0.4",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "setupFiles": [
      "./.jest.js"
    ],
    "modulePaths": [
      "./src"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.jest.js",
      "index.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.scss$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
